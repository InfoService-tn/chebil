<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

    <record id="l10n_tn_account_stamp_1_tnd" model="product.product">
        <field name="name">Tax Stamp 1 tnd</field>
        <field name="type">service</field>
        <field name="list_price" eval="1.0" />
        <field name="standard_price" eval="1.0" />
        <field
            name="description_sale"
        >Stamp duty payment under the terms of Art. 6, Subsection 2 of MEF Decree June
            17, 2014
        </field>
        <field name="is_stamp" eval="True" />
        <field name="auto_compute" eval="True" />
        <field name="stamp_apply_min_total_base" eval="0.001" />
        <field name="taxes_id" eval="[(6, 0, [])]" />
        <field name="supplier_taxes_id" eval="[(6, 0, [])]" />
    </record>

    <record id="base.main_company" model="res.company">
        <field
            name="tax_stamp_product_id"
            ref="l10n_tn_account_stamp.l10n_tn_account_stamp_1_tnd"
        />
    </record>

</odoo>
